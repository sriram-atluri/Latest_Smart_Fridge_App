<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activityClasses.AboutPage">

    <include layout="@layout/background_picture"/>
    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.MyApplication.AppBarOverlay"/>

    <include
        android:id="@+id/include"
        layout="@layout/content_about_page" />

    <include layout="@layout/reusable_layout_toolbar"/>
    <include layout="@layout/top_toolbar_layout_reusable"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/aboutActivityContainer">
    </FrameLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>