<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profileSettingsClassesAndFragments.ProfileSettingsActivity">

    <include layout="@layout/background_picture" />
    <include layout="@layout/reusable_layout_toolbar"/>

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.MyApplication.AppBarOverlay"
        android:translationY="675dp" />

    <FrameLayout
        android:id="@+id/profileActivityContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </FrameLayout>

    <!--<include layout="@layout/content_profile_screen" />-->


</androidx.coordinatorlayout.widget.CoordinatorLayout>